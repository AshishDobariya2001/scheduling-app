{
  "name": "kroolo-task",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "agenda": "^5.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "4.17.1",
    "express-basic-auth": "^1.2.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.16.5",
    "nodemailer": "^6.9.0",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.23.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "scripts": {
    "start": "npm run migrate:up && nodemon ./src/server.js",
    "seed": "node ./scripts/seedDb.js",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "sequelize-mock": "^0.10.2",
    "supertest": "^7.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js",
      "!src/config/config.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 35,
        "functions": 30,
        "lines": 50
      }
    }
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true
}
